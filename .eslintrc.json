{
  "extends": "next/core-web-vitals",
  "plugins": [
    "import"
  ],
  "rules": {
    "react/display-name": "off",
    "import/order": [
      "error",
      {
        "group": [
          "builtin",
          "external",
          "internal",
          "sibling",
          "parent",
          "index",
          "unknown"
        ]
      }
    ]
  }
}